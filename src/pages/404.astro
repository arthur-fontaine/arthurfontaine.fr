---
import redirects from "../../data/redirects.json";

const redirectsMapping = Object.fromEntries(
  redirects.redirects.map((redirect) => [redirect.from, redirect.to])
);

if (Astro.url.pathname in redirectsMapping) {
  return Astro.redirect(redirectsMapping[Astro.url.pathname]);
}

return Astro.redirect('/')
